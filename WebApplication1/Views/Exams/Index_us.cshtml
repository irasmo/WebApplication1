<br />
<div class="container row p-0 m-0">
    <div class="col-6">
        <h2 class="text-info">Exam List</h2>

    </div>
    <div class="col-2 offset-3">
        <a asp-action="Upsert" asp-controller="Exams" class="btn btn-info form-control text-white">
            Add New Exam
        </a>
        <p></p>
        <a asp-action="Logout" asp-controller="Users" class="btn btn-info form-control text-white">
            Log Out
        </a>
       <p></p>
        <a asp-action="Account" asp-controller="Users" class="btn btn-info form-control text-white">
            Account
        </a>
    </div>
    <div class="col-12 border p-3">
        <table id="DT_load" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>Surname</th>
                    <th>Subject</th>
                    <th>Grade</th>
                    <th>Speciality</th>
                    <th>Teacher</th>
                    <th></th>
                </tr>
            </thead>
        </table>
    </div>
</div>


